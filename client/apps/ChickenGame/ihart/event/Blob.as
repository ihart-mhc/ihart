package ihart.event{		/**	* Blob	* @author Felicia Cordeiro 11/23/2010	* @author Cleo Schneider	*	* Blob is a simple object which provides	* getters to allow the user the ability	* to get information the particular blob	**/	public class Blob {				/**		* Fields		**/		private var xCoor : Number;		private var yCoor : Number;		private var bWidth : Number;		private var bHeight : Number;		private var type : int;				/**		* Constructor		* sets all properties for a blob		*		* @param type The type of the blob		* @param xCoor The xCoordinate of the center of the blob		* @param yCoor The yCoordinate of the center of the blob		* @param bWidth The width of the blob		* @param bHeight The height of the blob		*		**/		public function Blob(type : int, xCoor : Number, yCoor : Number, bWidth : Number, bHeight : Number){			this.xCoor = xCoor;			this.yCoor = yCoor;			this.bWidth = bWidth;			this.bHeight = bHeight;			this.type = type;		}				/**		* Gets the xCoor for the center of the blob		* @return The xCoor for the center of the blob		**/		public function getX() : Number {			return xCoor;		}				/**		* Gets the yCoor for the center of the blob		* @return The yCoor for the center of the blob		**/		public function getY() : Number {			return yCoor;		}				/**		* Gets the width of the blob		* @return The width of the blob		**/		public function getWidth() : Number {			return bWidth;		}				/**		* Gets the height of the blob		* @return The height of the blob		**/		public function getHeight() : Number {			return bHeight		}		/**		* Gets the type of the blob		* @return The type of the blob as an String (using the constants from CVEvent)		**/		public function getType() : String{			return CVEvent.getStringType( type );		}	}}