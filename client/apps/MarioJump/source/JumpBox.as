package  {	import flash.display.Stage;	import flash.display.MovieClip;	import flash.utils.Timer;	//import ihart.event.*;	import flash.events.*;	public class JumpBox extends MovieClip{				private var timeHit:int;		private var mainGame:MainGame;		public function JumpBox(main:MainGame) {			// constructor code						addEventListener(MouseEvent.CLICK, boxClicked);			trace("hello box");			mainGame = main;					}				function boxClicked(e: MouseEvent):void		{			timeHit++;			trace("i have been clicked "+timeHit+" times");			mainGame.addScore();		}			}	}