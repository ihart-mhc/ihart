package ihart.event{		/**	* @author Felicia Cordeiro 11/23/2010	* @author Cleo Schneider	*	* Blob is a simple object which provides	* getters and setters and allow the user the ability	* to save information for a particular blob	**/	public class Blob {				/**		* Fields		**/		private var xCoor : Number;		private var yCoor : Number;		private var bWidth : Number;		private var bHeight : Number;		private var type : String;		private var roi : int; // region of interest index				/**		* Constructor		* sets all properties for a blob		**/		public function Blob(eventType : String, xCoor : Number, yCoor : Number, bWidth : Number, bHeight : Number, roi : int){			this.xCoor = xCoor;			this.yCoor = yCoor;			this.bWidth = bWidth;			this.bHeight = bHeight;			type = eventType;			this.roi = roi;		}				/**		* Get region of interest index		* return roi		**/		public function getROI() : int {			return roi;		}				/**		* Get x		* return xCoor		**/		public function getX() : Number {			return xCoor;		}				/**		* Get y		* return yCoor		**/		public function getY() : Number {			return yCoor;		}				/**		* Get width		* return width		**/		public function getWidth() : Number {			return bWidth;		}				/**		* Get height		* return height		**/		public function getHeight() : Number {			return bHeight		}		/**		* getType		**/		public function getType() : String{			return type;		}	}}