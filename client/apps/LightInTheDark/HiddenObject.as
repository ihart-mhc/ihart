package  {	import flash.display.MovieClip;		public class HiddenObject {				//name		var objName:String = "";				//the symbol that represents this object.		public var theObject:MovieClip;				//whether or not this object has been found or not		private var found:Boolean;				//number of points this object is worth		private var points:int;				public function HiddenObject(theObj:MovieClip, objectName:String, pointValue:int) {			theObject = theObj;			points = pointValue;			found = false;			objName = objectName;		}				public function setFound():void{			found = true;		}				public function getName():String{			return objName;		}				public function getPoints():int{			return points;		}				public function getX():Number{			return theObject.x;		}				public function getY():Number{			return theObject.y;		}	}	}