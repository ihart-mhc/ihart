package {	import flash.events.MouseEvent;	import ihart.event.*;	import flash.display.MovieClip;	import flash.display.DisplayObjectContainer;	import flash.text.TextField;		public class QuizFinalViewController extends MovieClip {				//create a cvManager		var cv_man : CVManager;				//The QuizController object, which we need to go back to the first screen.		var quizController:QuizController;		var returnButton : Button;				//Number of questions the person got right		var correctCount:int;				public function QuizFinalViewController(cCount:int, quizCont:QuizController, cvman:CVManager) {			correctCount = cCount;			quizController = quizCont;			cv_man = cvman;						scoreText.text = (correctCount/5 * 100) + "%";		}				public function displayScreen():void {			//Display the button...			returnButton = new Button();			var doc:DisplayObjectContainer = returnButton.upState as DisplayObjectContainer;			var tf:TextField = doc.getChildAt(1) as TextField;			tf.text = "Return to Main";			tf.autoSize = "center";						doc = returnButton.downState as DisplayObjectContainer;			tf = doc.getChildAt(1) as TextField;			tf.text = "Return to Main";			tf.textColor = 0xAAAAAA;			tf.autoSize = "center";						doc = returnButton.overState as DisplayObjectContainer;			tf = doc.getChildAt(1) as TextField;			tf.text = "Return to Main";			tf.textColor = 0xAAAAAA;			tf.autoSize = "center";						returnButton.x = -100;			returnButton.y = 200;			returnButton.setCVManager(cv_man);			returnButton.enable(CVEvent.SHELL);			returnButton.addEventListener(MouseEvent.CLICK, returnButtonClicked);			addChild(returnButton);		}				public function remove():void {			//Removes the button from the screen.			removeEventListener(MouseEvent.CLICK, returnButtonClicked);						removeChild(returnButton);		}				public function returnButtonClicked(e:MouseEvent):void {			quizController.returnToMainScreen();		}	}}